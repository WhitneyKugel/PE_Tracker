<!DOCTYPE html>
<html lang="en">
<head>
  <title>DB INFO</title>
  <meta charset="UTF-8">
</head>
<body>
  <h1>Database Table</h1>
  <ul>
    <% tables.forEach(function(r) { %>
      <li><%= r.table %>: <%= r.column %> (<%= r.type %>)</li>
    <% }); %>
  </ul>
  <h1>Observations</h1>
  <ul>
    <% obs.forEach(function(r) { %>
      <li><%= r.tasks_id %> for <%= r.duration || "00" %></li>
    <% }); %>
  </ul>
</body>
</html>